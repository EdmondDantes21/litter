  <version>0.0.0</version>
  <description>ros2 bdi core nodes and webots controllers communication</description>
  <maintainer email="redi.vreto@studenti.unitn.it">Redi</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_action</depend>

  <depend>plansys2_msgs</depend>
  <depend>plansys2_executor</depend>
  <depend>plansys2_problem_expert</depend>

  <depend>ros2_bdi_utils</depend>
  <depend>ros2_bdi_interfaces</depend>
  <depend>ros2_bdi_skills</depend>
  <depend>ros2_bdi_core</depend>
  
  <depend>javaff_interfaces</depend>  

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
